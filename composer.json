{
    "name": "plixus/twig-component-preview-bundle",
    "description": "Symfony bundle for generating live preview interfaces for Twig Components with interactive forms and real-time updates",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "twig", "components", "live-components", "preview", "ui", "development", "tools"],
    "homepage": "https://github.com/plixus/twig-component-preview-bundle",
    "support": {
        "issues": "https://github.com/plixus/twig-component-preview-bundle/issues",
        "source": "https://github.com/plixus/twig-component-preview-bundle"
    },
    "authors": [
        {
            "name": "Plixus",
            "email": "dev@plixus.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/twig-bundle": "^6.4|^7.0",
        "symfony/ux-twig-component": "^2.8",
        "symfony/ux-live-component": "^2.8",
        "symfony/stimulus-bundle": "^2.0",
        "symfony/asset-mapper": "^6.4|^7.0",
        "symfony/asset": "^6.4|^7.0",
        "symfony/twig-pack": "^1.0",
        "symfony/form": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0",
        "symfony/phpunit-bridge": "^6.4|^7.0",
        "symfony/browser-kit": "^6.4|^7.0",
        "symfony/css-selector": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Plixus\\TwigComponentPreviewBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Plixus\\TwigComponentPreviewBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "require": "^6.4|^7.0"
        }
    }
}